function openPopup(id, CloseOnModal, CloseApply) {
    $('#' + id).bPopup({
        modalClose: CloseOnModal,
        position: ['auto', 'auto'],
        closeClass: 'b-close',
        easing: 'easeOutBack',
        speed: 800,
        transition: 'slideDown',
        onOpen: function() {
            //OverFlowBody('hidden'); 
        },
        onClose: function() {
            //OverFlowBody(''); 
        $('#ctl00_ctl00_hfInprocessFlag').val(0); }
    }).center();

    if (CloseApply == true) {
        $('.b-close').show();
    }
    else if (CloseApply == false) {
        $('.b-close').hide();
    }
    return false;
}
function closePopup(id) {
    $('#' + id).bPopup().close();
}

